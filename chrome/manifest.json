{
  "name": "Tapmo.co",
  "version": "0.1",
  "manifest_version": 2,
  "description": "The official extension of Tapmo.co.",
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/", "https://*/"],
      "js": ["script.js"]
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "notifications",
    "http://*/"
  ],
  "background": {
    "page": "popup.html"
  }
}